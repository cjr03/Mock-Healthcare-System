{
  "name": "infusion-tracker",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/server.js",
    "openapi:gen": "openapi-generator-cli generate -i openapi/infusion-api.yaml -g typescript-axios -o src/client --skip-validate-spec",
    "demo:client": "ts-node-dev --transpile-only src/integrations/demoClient.ts",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-openapi-validator": "^5.5.8",
    "js-yaml": "^4.1.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.22.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
